{
  "name": "campu5",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:export": "next build",
    "build:mobile": "next build && npx cap sync",
    "start": "next start",
    "lint": "eslint",
    "clean": "rm -rf .next out",
    "clean:build": "npm run clean && npm run build:export",
    "clean:mobile": "npm run clean && npm run build:mobile",
    "sync:android": "npx cap sync android",
    "sync:ios": "npx cap sync ios",
    "sync": "npx cap sync",
    "open:android": "npx cap open android",
    "open:ios": "npx cap open ios",
    "android:dev": "npm run build:mobile && npm run open:android",
    "ios:dev": "npm run build:mobile && npm run open:ios",
    "debug:android": "npm run clean:mobile && npm run open:android",
    "debug:ios": "npm run clean:mobile && npm run open:ios",
    "android:run": "npx cap run android",
    "ios:run": "npx cap run ios",
    "update:capacitor": "npm install @capacitor/core@latest @capacitor/cli@latest",
    "doctor": "npx cap doctor"
  },
  "dependencies": {
    "@capacitor/app": "^7.1.0",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@headlessui/react": "^2.2.8",
    "@supabase/supabase-js": "^2.57.4",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "5.9.3"
  }
}
